    "version": "0.2.0",
    "configurations": [
        {
            "name": "Example Config, Debug Mode",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/jellytest/main.go",
            "cwd": "${workspaceFolder}",
            "args": [
                "-c",
                "${workspaceFolder}/config.example.yml"
            ]
        },
        {
            "name": "Live Config, Debug Mode",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/cmd/jellytest/main.go",
            "cwd": "${workspaceFolder}",
            "args": [
                "-c",
                "${workspaceFolder}/cmd/jellytest/jelly.yml"
            ]
        },
        {
            "name": "Current File Tests In Debug Mode",
            "type": "go",
            "request": "launch",
            "mode": "test",
            "program": "${workspaceFolder}/${relativeFileDirname}",
            "showLog": true
        }
    ]
}